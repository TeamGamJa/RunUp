  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="running">
   
   <select id="selectCategoryBigList" resultType="CategoryBig">
    <![CDATA[
      select * from categorybig
    ]]>  
   </select>
   
